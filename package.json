{
	"name": "feed-n-type",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && oxlint && eslint .",
		"lint:oxlint": "oxlint",
		"lint:eslint": "eslint .",
		"test": "npm run test:e2e",
		"test:e2e": "playwright test",
		"deploy": "pnpm run build && wrangler pages deploy",
		"preview:cf": "pnpm run build && wrangler pages dev",
		"cf:tail": "wrangler pages deployment tail"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250826.0",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.34.0",
		"@fontsource/fira-mono": "^5.2.6",
		"@neoconfetti/svelte": "^2.2.2",
		"@oxlint/migrate": "^1.13.0",
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-oxlint": "^1.13.0",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"oxlint": "^1.13.0",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.3",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
		"vite": "^7.1.3",
		"wrangler": "^4.32.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
